		"@types/winston": "^2.4.4",
		"prisma": "^4.4.0",
		"source-map-support": "^0.5.21",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"@prisma/client": "^4.4.0",
		"cors": "^2.8.5",
